require 'irb'

def sexp
  ruby      = File.readlines(FIXTURE_ROOT.join 'report.rb').join
  parser    = RubyParser.new
  ruby2ruby = Ruby2Ruby.new
  parser.process(ruby)
  # ruby2ruby.process(sexp)
end

def sexp_spec
  ruby      = File.readlines(FIXTURE_ROOT.join 'report_spec.rb').join
  parser    = RubyParser.new
  ruby2ruby = Ruby2Ruby.new
  parser.process(ruby)
  # ruby2ruby.process(sexp)
end

IRB.start
